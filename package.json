{
  "name": "backbone-sample",
  "private": true,
  "version": "0.0.1",
  "description": "Backbone Sample",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyup -u 2 ./src/lib/**/* ./dist/lib && copyup -u 2 ./src/lib/* ./dist/lib && copyup -u 2 ./src/images/**/* ./dist/images",
    "copy2Webapp": "copyup ./dist/**/* ../webapp",
    "dist": "npm run copy && webpack --env=dist && npm run copy2Webapp",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "go": "node server.js --env=dev",
    "start": "webpack-dev-server --host 0.0.0.0 --env=dev --config webpack.config.js",
    "prestart": "node open.js",
    "server:webpack": "webpack-dev-server --inline --config webpack.config.js --progress --colors --watch --display-modules",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "flow": "flow check || exit 0",
    "flowStatus": "flow status"
  },
  "repository": "",
  "keywords": [],
  "author": "Your name here",
